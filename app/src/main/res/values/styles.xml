<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ToolbarTitle" parent="TextAppearance.Material3.TitleLarge">
        <item name="android:textColor">@color/on_surface</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="FabCircle" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="MuscleChip" parent="Widget.Material3.Chip.Filter">
        <item name="chipBackgroundColor">@color/chip_background_color</item>
        <item name="chipCornerRadius">10dp</item>
        <item name="android:textColor">@color/chip_text_color</item>
        <item name="chipStrokeColor">@color/chip_stroke_color</item>
        <item name="chipStrokeWidth">1dp</item>
        <item name="checkedIconVisible">false</item>
    </style>

</resources>
